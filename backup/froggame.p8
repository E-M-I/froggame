pico-8 cartridge // http://www.pico-8.com
version 18
__lua__
x=8
y=106
spriteflip = nil

playerwalk = false
walkloop = false
imagenmb = 1
clock = 0

function _update()
	if (clock>=30) then
		clock = 0
	
	else clock = clock + 1 end

	if (btn(0) or btn(1)) then
		spriteflip = true
		playerwalk = true
		walkloop = true
		if (btn(0)) then 
			x = x - 1 
			spriteflip = true
		end
		if (btn(1)) then 
			x = x + 1
			spriteflip = false
	 end
	end
	
	if (playerwalk==true) then
		if (clock==15) then 
				imagenmb = 2
	
		elseif (clock==30) then
			imagenmb = 1 
		end
	end
	
end  

function _draw()
	cls()
 rectfill(0,0,130,130,12)

	for f=0,15,1 do
		sspr(40,0,40,16,40*f,98)
		spr(17,8*f,114,1,1)
		spr(33,8*f,122,1,1)
	end 
	
	spr(imagenmb,x,y,1,1,spriteflip)
end
