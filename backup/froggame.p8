pico-8 cartridge // http://www.pico-8.com
version 18
__lua__
x=8
y=98
spriteflip = false

playerwalk = false
walkloop = false

imagenmb = 0
imagespd = 5
clock = 0
clockframe = clock + imagespd

function _update()
	if (clock>=30) then
		clock = 0
		clockframe = clock + imagespd
	
	else clock = clock + 1 end
	
if (clock==clockframe) then
	if (playerwalk==true) then
			
		if (imagenmb<2) then
			imagenmb = imagenmb + 1
				
		else imagenmb = 1 end
	
	else imagenmb = 0 end
		
	clockframe = clock + imagespd
end

	if (btn(0) or btn(1)) then
		spriteflip = true
		playerwalk = true
		walkloop = true
		if (btn(0)) then 
			x = x - 1 
			spriteflip = true
		end
		if (btn(1)) then 
			x = x + 1
			spriteflip = false
	 end
	
	else playerwalk = false end
	
end  

function _draw()
	cls()
 rectfill(0,0,130,130,12)

	for f=0,15,1 do
		sspr(48,0,40,16,40*f,98)
		spr(32,8*f,114)
		spr(33,8*f,122)
	end 
	
	sspr(0+(16*imagenmb),0,16,16,x,y,16,16,spriteflip) 
end
